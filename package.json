{
  "name": "city-energy-analyst-gui",
  "productName": "CityEnergyAnalyst-GUI",
  "version": "3.32.0",
  "description": "Official GUI of the City Energy Analyst tool",
  "homepage": "http://www.cityenergyanalyst.com",
  "repository": "github:architecture-building-systems/CityEnergyAnalyst-GUI",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "dev-electron": "vite --mode electron",
    "build-electron": "vite build --mode electron",
    "preview": "vite preview",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "electron:dev": "concurrently \"yarn dev-electron\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "yarn build-electron && electron-builder build --publish never",
    "electron:release": "electron-builder"
  },
  "keywords": [],
  "author": {
    "name": "Architecture and Building Systems",
    "email": "cea@arch.ethz.ch"
  },
  "maintainers": [
    {
      "name": "Reynold Mok",
      "email": "reynold.mok@arch.ethz.ch"
    }
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "concurrently": "^8.2.2",
    "electron": "^28.2.3",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "prettier": "^3.2.5",
    "redux-logger": "^3.0.6",
    "vite": "^5.1.3",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@ant-design/compatible": "^5.1.2",
    "@ant-design/icons": "^5.3.0",
    "@babel/polyfill": "^7.12.1",
    "@handsontable/react": "^3.1.2",
    "@react-spring/web": "^9.7.3",
    "@turf/turf": "^6.5.0",
    "antd": "^5.14.1",
    "axios": "^1.6.7",
    "color-interpolate": "^1.0.5",
    "connected-react-router": "^6.5.2",
    "deck.gl": "^8.9.34",
    "electron-log": "^5.1.2",
    "electron-updater": "^6.1.8",
    "events": "^3.3.0",
    "handsontable": "^6",
    "history": "^4.9.0",
    "html-react-parser": "^3.0.4",
    "maplibre-gl": "^4.0.0",
    "nebula.gl": "^1.0.4",
    "path-browserify": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-map-gl": "^7.1.7",
    "react-redux": "^8.0.5",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.2",
    "socket.io-client": "^4.5.3",
    "tabulator-tables": "^4.4.1"
  },
  "main": "electron/main.mjs",
  "build": {
    "appId": "CityEnergyAnalyst-GUI",
    "directories": {
      "output": "out"
    },
    "files": [
      "dist/**/*",
      "electron/**",
      "!node_modules/**/*"
    ],
    "mac": {
      "icon": "public/cea.icns",
      "target": [
        "dmg"
      ],
      "extraResources": [
        {
          "from": "dependencies",
          "to": ".",
          "filter": [
            "**/*",
            "!**/__pycache__"
          ]
        }
      ]
    },
    "win": {
      "icon": "public/cea.ico",
      "target": [
        "dir"
      ]
    }
  }
}
