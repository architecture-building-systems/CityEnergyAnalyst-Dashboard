{
  "name": "city-energy-analyst-gui",
  "productName": "CityEnergyAnalyst-GUI",
  "version": "3.26.0",
  "description": "Official GUI of the City Energy Analyst tool",
  "homepage": "http://www.cityenergyanalyst.com",
  "repository": "github:architecture-building-systems/CityEnergyAnalyst-GUI",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "yarn eslint ."
  },
  "keywords": [],
  "author": {
    "name": "Reynold Mok",
    "email": "reyold@hotmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": true,
        "icon": "build/icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "devContentSecurityPolicy": "default-src * self blob: data: gap:; style-src * self 'unsafe-inline' blob: data: gap:; script-src * 'self' 'unsafe-eval' 'unsafe-inline' blob: data: gap:; object-src * 'self' blob: data: gap:; img-src * self 'unsafe-inline' blob: data: gap:; connect-src self * 'unsafe-inline' blob: data: gap:; frame-src * self blob: data: gap:;",
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "nodeIntegration": true,
              "contextIsolation": false,
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "name": "main_window",
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.js"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.7",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-react": "^7.14.5",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.0.0",
    "electron": "17.1.2",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "node-loader": "^2.0.0",
    "prettier": "^2.4.1",
    "redux-logger": "^3.0.6",
    "style-loader": "^3.0.0"
  },
  "dependencies": {
    "@ant-design/compatible": "^1.1.0",
    "@ant-design/icons": "^4.7.0",
    "@handsontable/react": "^3.1.2",
    "@turf/turf": "^6.5.0",
    "antd": "^4.19.2",
    "axios": "0.26.1",
    "color-interpolate": "^1.0.5",
    "connected-react-router": "^6.5.2",
    "deck.gl": "7",
    "electron-log": "^4.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "frameless-titlebar": "^2.1.4",
    "handsontable": "6",
    "history": "^4.9.0",
    "html-react-parser": "^0.9.1",
    "ini": "^2.0.0",
    "nebula.gl": "0.15.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-map-gl": "5",
    "react-redux": "^7.2.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-spring": "^9.2.5",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "socket.io-client": "^2.3.0",
    "tabulator-tables": "^4.4.1"
  }
}
